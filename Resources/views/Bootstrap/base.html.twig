<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <title>{% block title %}{{ 'Welcome!'|trans }}{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {% block metas %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('bundles/merobase/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/merobase/css/font-roboto.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/merobase/css/material-icons.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/merobase/css/material-color-palette.min.css') }}" rel="stylesheet" type="text/css"/>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="{{ asset('bundles/merobase/js/jquery.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/merobase/js/bootstrap.min.js') }}" type="text/javascript"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
